{
  "expo": {
    "name": "look-match",
    "slug": "look-match",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "lookmatch",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nicolasinfante.lookmatch"
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nicolasinfante.lookmatch"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "367f1709-dad6-4fa1-8a8e-a443694a5e49"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci칩n necesita acceso a tus fotos.",
          "cameraPermission": "La aplicaci칩n necesita acceso a la c치mara."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Necesitamos acceso a tu c치mara para analizar tu estilo."
        }
      ],
      "expo-secure-store"
    ]
  }
}